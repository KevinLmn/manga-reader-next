{
  "$schema": "https://unpkg.com/knip@5.53.0/schema.json",
  "ignore": [
    "**/*.test.ts",
    "**/test/**",
    "**/__tests__/**",
    "**/prisma/**",
    "**/dist/**",
    "**/.next/**"
  ],
  "entry": [
    "frontend/app/**/*.ts",
    "frontend/app/**/*.tsx",
    "frontend/features/**/*.ts",
    "frontend/features/**/*.tsx",
    "frontend/middleware.ts",
    "backend/src/**/*.ts"
  ],
  "workspaces": {
    "frontend": {},
    "backend": {}
  }
}
